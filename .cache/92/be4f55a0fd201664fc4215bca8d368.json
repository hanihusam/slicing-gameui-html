, sans-serif;\n  color: #2222;\n  background-color: #ffffff;\n  width: 100%;\n  height: 100%;\n  font-size: 14px;\n  position: relative;\n  z-index: 1;\n}\nbody > div,\nbody > section,\nbody > header,\nbody > footer {\n  overflow-x: hidden;\n}\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  font-weight: 700;\n  color: #232323;\n  line-height: 1.2;\n}\nul,\nol {\n  margin: 0;\n  padding: 0;\n}\na,\na:hover {\n  color: #ffffff;\n}\na,\na:hover,\na:focus,\na:active,\nbutton,\nbutton:hover {\n  text-decoration: none;\n  touch-action: manipulation;\n  -webkit-transition-duration: 500ms;\n  -o-transition-duration: 500ms;\n  transition-duration: 500ms;\n}\nli {\n  list-style: none;\n}\np,\nlabel {\n  font-size: 14px;\n  color: #232323;\n}\n\n/* Navbar */\n.navbar {\n  padding: 0 1rem;\n}\n.navbar p {\n  color: #ffffff;\n}\n.navbar-collapse .nav-link {\n  margin-bottom: 5px;\n}\n.avatar {\n  position: relative;\n  width: 2rem;\n  height: 2rem;\n  font-size: 0.75rem;\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  font-weight: 400;\n  color: #6e7582;\n  text-align: center;\n  text-transform: uppercase;\n  vertical-align: bottom;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  -ms-user-select: none;\n  user-select: none;\n  background: #f1f3f8 no-repeat center/cover;\n  background-image: url(../img/default-avatar.png);\n  border-radius: 50%;\n}\n.hamburger {\n  display: flex;\n  flex-direction: column;\n  height: 20px;\n  justify-content: space-between;\n  position: relative;\n}\n.hamburger input {\n  position: absolute;\n  width: 40px;\n  height: 28px;\n  left: -5px;\n  top: -3px;\n  opacity: 0;\n  cursor: pointer;\n  z-index: 5;\n}\n.hamburger span {\n  display: block;\n  width: 28px;\n  height: 3px;\n  background-color: #ffffff;\n  border-radius: 3px;\n  transition: all 500ms;\n}\n.hamburger span:nth-child(2) {\n  transform-origin: 0 0;\n}\n.hamburger span:nth-child(4) {\n  transform-origin: 0 100%;\n}\n.hamburger input:checked ~ span:nth-child(2) {\n  background-color: #f1f3f8;\n  transform: rotate(45deg) translate(-1px, -1px);\n}\n.hamburger input:checked ~ span:nth-child(3) {\n  opacity: 0;\n  transform: scale(0);\n}\n.hamburger input:checked ~ span:nth-child(4) {\n  background-color: #f1f3f8;\n  transform: rotate(-45deg) translate(-1px, 0);\n}\nnav ul {\n  position: absolute;\n  right: 0;\n  top: 0;\n  height: 100vh;\n  flex-direction: column !important;\n  background-color: #343a40;\n  width: 100%;\n  transform: translateX(100%);\n  transition: all 1s;\n  opacity: 0;\n  z-index: -1;\n  padding-top: 60px;\n}\nnav ul.slide {\n  transform: translateX(0);\n  opacity: 1;\n}\nnav ul .nav-item {\n  padding: 15px;\n  border-bottom: thin solid #6e7582;\n}\n\n/* DESKTOP VERSION */\n/* Navbar */\n@media screen and (min-width: 992px) {\n  nav .nav-link {\n    margin-bottom: 0;\n  }\n  nav ul {\n    flex-direction: row !important;\n    position: unset;\n    height: auto;\n    width: auto;\n    transform: unset;\n    opacity: 1;\n    z-index: auto;\n    padding: unset;\n  }\n  nav ul .nav-item,\n  .nav-item.outside {\n    padding: unset;\n    border-right: thin solid #6e7582;\n    border-bottom: unset;\n  }\n  .nav-link {\n    margin-right: 15px;\n    margin-left: 15px;\n    margin-bottom: 0;\n  }\n  .hamburger {\n    display: none;\n  }\n  .nav-item.active::after {\n    content: \"\";\n    display: block;\n    margin: auto;\n    margin-bottom: -18px;\n    border-bottom: 3px solid #ffd31d;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ce6208f591231cc08f7bb101b821ad3e","cacheData":{"env":{}}}